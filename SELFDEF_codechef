#include <stdio.h>

int main() 
{
	int T;
	int N;
	int i;
	
	scanf("%d",&T);
	while(T-->0){
	    scanf("%d",&N);
	    int A[N],MAX;
	    for(int i=0;i<N;i++){
	        scanf("%d",&A[i]);
	    }
	        int count=0;
	    for(int i=0;i<N;i++){
	        if(A[i]>=30 && A[i]<=60){
	            count=count+1;
	        }
	        
	     
	    }
	    printf("%d\n",count);
	   
	}
	return 0;
}
