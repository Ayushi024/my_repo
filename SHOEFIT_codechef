#include <stdio.h>

int main() 
{
	int T;
	int A,B,C;
	scanf("%d",&T);
	while(T-->0){
	    scanf("%d %d %d",&A,&B,&C);
	    if(A==0 || B==0 || C==0)
	    {
	        if(B==1 || C==1 || A==1)
	        {
	            printf("1\n");
	        }
	        else if( A==0 && B==0 && C==0)
	        {
	      printf("0\n");
	  
	        }
	    }
	  else if(A==1 && B==1 && C==1)

	  {   
	      printf("0\n");
	  }  
	}
	
	return 0;
}

