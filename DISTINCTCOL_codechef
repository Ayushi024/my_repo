#include <stdio.h>

int main(void) {
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n;
        scanf("%d",&n);
        int a[n],i,max;
        for(i=0;i<n;i++)
        scanf("%d",&a[i]);
        max=a[0];
        for(i=0;i<n;i++)
        {
            if(a[i]>max)
            max=a[i];
        }
        printf("%d\n",max);
    }

	return 0;
}
