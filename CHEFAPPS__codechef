#include <stdio.h>

int main() 
{
	int T;
	int S;
	int A,B,Z,n;
	int sum,total;
	scanf("%d",&T);
	while(T-->0){
	    scanf("%d %d %d %d",&S,&A,&B,&Z);
	    total=A+B+Z;
	    if(total<=S){
	        printf("0\n");
	    }
	    else{
	        if(Z+A<=S)
	        {
	            printf("1\n");
	        }
	        else{
	            printf("2\n");
	        }
	    }
	    
	    
	}
	return 0;
}
